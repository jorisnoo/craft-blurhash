{
    "name": "dodecastudio/blurhash",
    "description": "Render a BlurHash from a given image.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "blurhash"
    ],
    "support": {
        "docs": "https://github.com/dodecastudio/craft-blurhash.git/blob/master/README.md",
        "issues": "https://github.com/dodecastudio/craft-blurhash.git/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dodeca Studio",
            "homepage": "https://dodeca.studio"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "kornrunner/blurhash": "^1.2",
        "treeware/plant": "^0.1.0"
    },
    "require-dev": {
        "craftcms/cms": "^3.2.0",
        "codeception/codeception": "^3.0",
        "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "dodecastudio\\blurhash\\": "src/"
        }
    },
    "extra": {
        "treeware": {},
        "name": "BlurHash",
        "handle": "blur-hash",
        "developer": "Dodeca Studio",
        "developerUrl": "https://dodeca.studio",
        "documentationUrl": "https://github.com/github.com/dodecastudio/craft-blurhash.git/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/github.com/dodecastudio/craft-blurhash.git/master/CHANGELOG.md",
        "class": "dodecastudio\\blurhash\\BlurHash"
    }
}